#include <iostream>

using namespace std;

int LCS[107][107];

int main() {
    char a[20], b[20];
    int n1, n2;
    cin>>n1>>n2;
    for(int i=0; i<n1; i++)
        cin>>a[i];
    for(int i=0; i<n2; i++)
        cin>>b[i];

    /** Algorithm part. **/
    for(int i=0; i<=n1; i++)
    {
        for(int j=0; j<=n2; j++)
        {
            if(i==0 || j==0){
                LCS[i][j] = 0;
            }
            if(i>0 && j>0)
            {
                if(a[i] == b[j])
                    LCS[i][j] = LCS[i-1][j-1] + 1;
                else
                    LCS[i][j] = max(LCS[i-1][j], LCS[i][j-1]);
            }
        }
    }
    /** Print the LCS double array. **/
    for(int i=0; i<=n1; i++)
    {
        for(int j=0; j<=n2; j++)
            cout<<LCS[i][j]<<" ";
        cout<<endl;
    }
    
    /** Print the LCS length. **/
    cout<<LCS[n1-1][n2-1];

    return 0;
}
